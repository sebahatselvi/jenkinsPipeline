pipeline{
    agent any  
    stages{
        stage("Git Checkout"){
            steps{
                git credentialsId: 'github', url: 'https://github.com/sebahatselvi/jenkinsPipeline'
            }
        }
        
        stage("Maven Build"){
        step{
          maven_invoker invokerBuildDir: 'target/it', reportsFilenamePattern: 'target/invoker-reports/BUILD*.xml'
        }
        }
        
    }
}
